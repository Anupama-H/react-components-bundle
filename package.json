  "license": "ISC",
  "sideEffects": false,
  "main": "lib/index.js",
  "scripts": {
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook",
    "watch-build": "babel src -d lib --watch",
